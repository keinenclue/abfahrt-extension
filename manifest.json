{
    "name": "Abfahrt Extension",
    "description": "Abfahrt!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting"
    ],  
    "content_scripts": [
        {
          "matches": ["https://*/*", "http://*/*"],
          "run_at": "document_end", 
          "js": ["background.js"]
        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" 
    }
}